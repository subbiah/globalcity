<h1>Listing township_masters</h1>

<table>
  <thead>
    <tr>
      <th>Townshipname</th>
      <th>Activeflag</th>
      <th>Address1</th>
      <th>Address2</th>
      <th>Locationcode</th>
      <th>Citycode</th>
      <th>Districtcode</th>
      <th>Satecode</th>
      <th>Countrycode</th>
      <th>Pincode</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @township_masters.each do |township_master| %>
      <tr>
        <td><%= township_master.townshipname %></td>
        <td><%= township_master.activeflag %></td>
        <td><%= township_master.address1 %></td>
        <td><%= township_master.address2 %></td>
        <td><%= township_master.locationcode %></td>
        <td><%= township_master.citycode %></td>
        <td><%= township_master.districtcode %></td>
        <td><%= township_master.satecode %></td>
        <td><%= township_master.countrycode %></td>
        <td><%= township_master.pincode %></td>
        <td><%= link_to 'Show', township_master %></td>
        <td><%= link_to 'Edit', edit_township_master_path(township_master) %></td>
        <td><%= link_to 'Destroy', township_master, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Township master', new_township_master_path %>
